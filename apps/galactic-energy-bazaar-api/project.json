{
  "name": "galactic-energy-bazaar-api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/galactic-energy-bazaar-api/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/galactic-energy-bazaar-api",
        "main": "apps/galactic-energy-bazaar-api/src/main.ts",
        "tsConfig": "apps/galactic-energy-bazaar-api/tsconfig.app.json",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/galactic-energy-bazaar-api/src/assets",
            "output": "assets"
          },
          {
            "glob": "vercel.json",
            "input": ".",
            "output": "."
          }
        ]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "galactic-energy-bazaar-api:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "galactic-energy-bazaar-api:build:development"
        },
        "production": {
          "buildTarget": "galactic-energy-bazaar-api:build:production"
        }
      }
    },
    "test": {
      "options": {
        "passWithNoTests": true
      }
    }
  }
}
